package com.ats.abongda.nvs.fragment.TAIKHOAN;

import java.util.ArrayList;
import java.util.List;

import android.os.Bundle;
import android.support.annotation.Nullable;
import android.view.LayoutInflater;
import android.view.View;
import android.view.ViewGroup;

import com.ats.abongda.nvs.activity.MainActivity;
import com.ats.abongda.nvs.fragment.BaseFragment;

public class TAIKHOAN_ProfileFragment extends BaseFragment {

	public TAIKHOAN_ProfileFragment(int mActionCode, List<Object> mArgs) {
		super(mActionCode, mArgs);
		// TODO Auto-generated constructor stub
	}

	public TAIKHOAN_ProfileFragment(List<Object> mArgs) {
		super(mArgs);
		// TODO Auto-generated constructor stub
	}

	@Override
	public View onCreateView(LayoutInflater inflater,
			@Nullable ViewGroup container, @Nullable Bundle savedInstanceState) {
		// TODO Auto-generated method stub
		if (!isSignedIn()) {
			((MainActivity)getRealActivity()).switchContent(SIGNIN, null);
		}
		return super.onCreateView(inflater, container, savedInstanceState);
	}
}
